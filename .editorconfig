# top-most EditorConfig file    bitnami/rabbitmq:latest rabbitmqctl -n rabbit@rabbitmq-server status
root = true

# Unix-style newlines with a newline ending every file
[*]
indent_size = 4
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.json]
indent_size = 2

[*.yml]
indent_size = 2
